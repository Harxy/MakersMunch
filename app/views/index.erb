<html>
  <head>
    <link rel='stylesheet' type='text/css' href='/styles/styles.css'>
    <script src="https://code.jquery.com/jquery-2.1.4.min.js"> </script>
    <script src="https://code.jquery.com/jquery-migrate-1.2.1.min.js"> </script>
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?libraries=places"></script>
  </head>
  <body class= "body">
    <h1 id="title"> Munch </h1>
  <%if current_user %>
  <form method='post' action='/log_in'>
    <input type='hidden' name='_method' value='delete'>
    <input type='submit' value='Sign out'>
  </form>
  <%else%>
    <a href="http://localhost:9292/user/new" class="button">Sign Up</a>
    <a href="http://localhost:9292/log_in" class="log_button">Log In</a>
  <%end%>

  <% if flash[:notice] %>
    <div id='notice'>
      <%= flash[:notice] %>
    </div>
  <% end %>
  <div id='map'> </div>
  <div id="boxed" style="overflow-y:scroll">
   <% @restaurants.each do |restaurant| %>

  <script src='googleApi.js'> </script>
      <script>
      var place_id = '<%= restaurant.place_id%>';
      getApiDetails(place_id);
      </script>
      <% end %>
      </div>
    </div>
  </div>

  </body>
</html>
